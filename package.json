{
  "name": "zap-gpt",
  "version": "2.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "config": "node ./setup.js",
    "dev": "tsup src/index.ts --watch --onSuccess 'node dist/index.cjs'",
    "build": "tsup src/index.ts --no-splitting",
    "start": "npm run build && pm2 start dist/index.cjs --name zap-gpt --no-daemon",
    "stop": "npx pm2 delete zap-gpt",
    "ob": "javascript-obfuscator dist/index.js",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org zap-gpt --project node ./dist/index.cjs && sentry-cli sourcemaps upload --org zap-gpt --project node ./dist/index.cjs"
  },
  "keywords": [],
  "author": {
    "name": "Giovanne Marrone",
    "email": "giovannemarrone@gmail.com"
  },
  "license": "ISC",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@google-cloud/logging-winston": "6.0.0",
    "@google/generative-ai": "0.3.1",
    "@hapi/boom": "10.0.1",
    "@whiskeysockets/baileys": "6.7.1",
    "axios": "1.6.8",
    "dotenv": "16.4.5",
    "fluent-ffmpeg": "^2.1.2",
    "inquirer": "9.2.16",
    "link-preview-js": "^3.0.5",
    "llamaindex": "^0.3.4",
    "node-cache": "5.1.2",
    "openai": "^4.46.1",
    "pino": "7.11.0",
    "qrcode-terminal": "0.12.0",
    "winston": "3.13.0"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/node": "^20.11.8",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "javascript-obfuscator": "^4.1.0",
    "pm2": "^5.3.1",
    "prettier": "^3.2.4",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
